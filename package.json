{
  "name": "vue-nuxt-snippets",
  "displayName": "Vue Ecosystem Snippets",
  "description": "Latest Vue Ecosystem snippets",
  "version": "0.0.1",
  "license": "MIT",
  "icon": "assets/icon.png",
  "author": {
    "name": "Matija Osreƒçki",
    "email": "matija.osrecki@gmail.com"
  },
  "publisher": "matijao",
  "engines": {
    "vscode": "^1.x.x"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "vue",
    "nuxt",
    "vue 3",
    "nuxt 3",
    "pinia",
    "volar",
    "vueuse",
    "composition api",
    "typescript",
    "snippets"
  ],
  "scripts": {
    "publish": "vsce package && vsce publish",
    "watch": "deno run -A --watch src/app.ts --snippets --docs",
    "generate": "deno run -A src/app.ts --snippets --docs",
    "generate:snippets": "deno run -A src/app.ts --snippets"
  },
  "contributes": {
    "snippets": [
      {
        "language": "vue",
        "path": "./dist/vue.code-snippets"
      },
      {
        "language": "html",
        "path": "./dist/vue-template.code-snippets"
      },
      {
        "language": "css",
        "path": "./dist/vue-style.code-snippets"
      },
      {
        "language": "postcss",
        "path": "./dist/vue-style.code-snippets"
      },
      {
        "language": "scss",
        "path": "./dist/vue-style.code-snippets"
      },
      {
        "language": "sass",
        "path": "./dist/vue-style.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./dist/vue-script.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./dist/vue-script.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./dist/vue-script.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./dist/vue-script.code-snippets"
      }
    ]
  }
}
