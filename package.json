{
  "name": "vue-nuxt-snippets",
  "displayName": "Vue Ecosystem Snippets",
  "description": "Latest Vue Ecosystem snippets - including Nuxt 3, VueUse, Pinia & Vue Router",
  "version": "0.0.1",
  "license": "MIT",
  "icon": "assets/icon.png",
  "author": {
    "name": "Matija Osreƒçki",
    "email": "matija.osrecki@gmail.com"
  },
  "publisher": "matijao",
  "repository": "https://github.com/mat2ja/vue-ecosystem-snippets",
  "bugs": "https://github.com/mat2ja/vue-ecosystem-snippets/issues",
  "engines": {
    "vscode": "^1.x.x"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "Vue",
    "Nuxt",
    "Vue 3",
    "Nuxt 3",
    "Pinia",
    "Volar",
    "VueUse",
    "Composition API",
    "TypeScript",
    "Vue Snippets",
    "Nuxt Snippets",
    "snippets",
    "snippet"
  ],
  "scripts": {
    "publish": "vsce package && vsce publish",
    "watch": "deno run -A --watch src/app.ts --snippets --docs",
    "generate": "deno run -A src/app.ts --snippets --docs",
    "generate:snippets": "deno run -A src/app.ts --snippets"
  },
  "contributes": {
    "snippets": [
      {
        "language": "vue",
        "path": "./dist/vue.code-snippets"
      },
      {
        "language": "html",
        "path": "./dist/vue-template.code-snippets"
      },
      {
        "language": "css",
        "path": "./dist/vue-style.code-snippets"
      },
      {
        "language": "postcss",
        "path": "./dist/vue-style.code-snippets"
      },
      {
        "language": "scss",
        "path": "./dist/vue-style.code-snippets"
      },
      {
        "language": "sass",
        "path": "./dist/vue-style.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./dist/vue-script.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./dist/vue-script.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./dist/vue-script.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./dist/vue-script.code-snippets"
      }
    ]
  }
}
